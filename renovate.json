{
  "extends": ["config:base", ":dependencyDashboard", "group:allNonMajor"],
  "ignorePaths": [
    "website/*",
    "docs/*",
    "./stage-*",
    "./test/*",
    "example/*",
    "templates/*",
    "conf/*"
  ],
  "pin": false,
  "rangeStrategy": "bump",
  "ignoreDeps": [
    "node",
    "typescript",
    "prettier",
    "jest",
    "eslint",
    "rollup",

    "react",
    "react-dom",
    "react-is",
    "ansi-escapes",
    "ansi-skip",
    "ora"
  ]
}
